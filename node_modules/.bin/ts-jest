#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ianderrington/git/supernal-nova/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@7.28.5_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_e08298c09311f3bd60009cf11487acb2/node_modules/ts-jest/node_modules:/Users/ianderrington/git/supernal-nova/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@7.28.5_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_e08298c09311f3bd60009cf11487acb2/node_modules:/Users/ianderrington/git/supernal-nova/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ianderrington/git/supernal-nova/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@7.28.5_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_e08298c09311f3bd60009cf11487acb2/node_modules/ts-jest/node_modules:/Users/ianderrington/git/supernal-nova/node_modules/.pnpm/ts-jest@29.4.5_@babel+core@7.28.5_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_e08298c09311f3bd60009cf11487acb2/node_modules:/Users/ianderrington/git/supernal-nova/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
